{
  "scripts": {
    "debug": "NODE_ENV=development node --inspect -r ts-node/register main.ts",
    "start": "ts-node src/index.ts",
    "test": "NODE_ENV=test jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:create": "ts-node scripts/db/create",
    "db:drop": "ts-node scripts/db/drop",
    "db:migrate": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:seed": "ts-node database/seeds.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.28"
  },
  "devDependencies": {
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "supertest": "^5.0.0",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333",
    "@types/node": "^8.0.29"
  }
}