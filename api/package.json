{
  "scripts": {
    "debug": "NODE_ENV=development nodemon --ext 'ts,js,json' --inspect --exec 'ts-node main.ts'",
    "test:unit": "NODE_ENV=test jest -c jest.config.unit.js",
    "test:integration": "NODE_ENV=test jest -c jest.config.integration.js --runInBand",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:create": "ts-node scripts/db/create",
    "db:drop": "ts-node scripts/db/drop",
    "db:migrate": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:setup": "npm run db:create && npm run db:migrate",
    "db:reset": "npm run db:drop && npm run db:setup",
    "db:seed": "ts-node database/seeds.ts"
  },
  "dependencies": {
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jira.js": "^1.7.2",
    "jstat": "^1.9.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.28"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^8.0.29",
    "@types/rosie": "0.0.38",
    "babel-jest": "^26.6.1",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "rosie": "^2.0.1",
    "supertest": "^5.0.0",
    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  }
}
