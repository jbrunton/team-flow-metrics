{
  "name": "metrics-api",
  "version": "0.1.0",
  "scripts": {
    "debug": "NODE_ENV=development nodemon --ext 'ts,js,json' --inspect --exec 'ts-node main.js'",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test jest -c jest.config.unit.js",
    "test:integration": "docker-compose --env-file=../.env run api npm run test:integration:local",
    "test:integration:local": "NODE_ENV=test jest -c jest.config.integration.js --runInBand",
    "pre-commit": "lint-staged",
    "lint": "eslint '**/*.ts' --fix --max-warnings 18",
    "lint:check": "eslint '**/*.ts' --max-warnings 18",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:create": "ts-node scripts/db/create",
    "db:drop": "ts-node scripts/db/drop",
    "db:migrate": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:setup": "npm run db:create && npm run db:migrate",
    "db:reset": "npm run db:drop && npm run db:setup",
    "db:seed": "ts-node database/seeds.ts"
  },
  "dependencies": {
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jira.js": "^2.3.0",
    "jstat": "^1.9.4",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.10",
    "seedrandom": "^3.0.5",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/async": "^3.2.6",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.166",
    "@types/luxon": "^1.25.0",
    "@types/node": "^15.6.1",
    "@types/rosie": "0.0.39",
    "@types/seedrandom": "^2.4.28",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "async": "^3.2.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^27.0.1",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rosie": "^2.0.1",
    "supertest": "^6.1.2",
    "ts-node": "10.0.0",
    "typescript": "~3.9.3"
  }
}
